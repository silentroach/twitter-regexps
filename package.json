{
  "name": "twitter-regexps",
  "version": "3.1.1",
  "description": "Twitter official precompiled regexps",
  "main": "index.js",
  "scripts": {
    "generate": "node src/generate",
    "doc": "node src/doc",
    "preversion": "git submodule update --recursive && npm run generate && npm run doc",
